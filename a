Tab:AddButton({
	Name = "Symphony Hub",
	Callback = function()
      		-- Instances:

	local Error = Instance.new("ScreenGui")
	local Error_2 = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local error_xlarge = Instance.new("ImageLabel")
	local UIToolsGradient = Instance.new("UIGradient")
	local Title404 = Instance.new("TextLabel")
	local Reason1 = Instance.new("TextLabel")
	local Reason = Instance.new("TextLabel")
	local Close = Instance.new("ImageButton")
	local TitleScript = Instance.new("TextLabel")

	--Properties:

	Error.Name = "Error"
	Error.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	Error.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	Error_2.Name = "Error"
	Error_2.Parent = Error
	Error_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Error_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Error_2.BorderSizePixel = 0
	Error_2.Position = UDim2.new(0.373097986, 0, 0.276829273, 0)
	Error_2.Size = UDim2.new(0, 460, 0, 245)

	UICorner.Parent = Error_2

	error_xlarge.Name = "error_xlarge"
	error_xlarge.Parent = Error_2
	error_xlarge.BackgroundTransparency = 1.000
	error_xlarge.Size = UDim2.new(0, 70, 0, 70)
	error_xlarge.Image = "rbxassetid://14755017532"
	error_xlarge.ImageRectOffset = Vector2.new(402, 200)
	error_xlarge.ImageRectSize = Vector2.new(96, 96)

	UIToolsGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(62, 147, 252)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(239, 183, 192))}
	UIToolsGradient.Name = "UIToolsGradient"
	UIToolsGradient.Parent = Error_2

	Title404.Name = "Title 404"
	Title404.Parent = Error_2
	Title404.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Title404.BackgroundTransparency = 1.000
	Title404.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Title404.BorderSizePixel = 0
	Title404.Position = UDim2.new(0.258695662, 0, 0, 0)
	Title404.Size = UDim2.new(0, 200, 0, 50)
	Title404.Font = Enum.Font.SourceSans
	Title404.Text = "Error 404"
	Title404.TextColor3 = Color3.fromRGB(0, 0, 0)
	Title404.TextScaled = true
	Title404.TextSize = 14.000
	Title404.TextWrapped = true

	Reason1.Name = "Reason1"
	Reason1.Parent = Error_2
	Reason1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Reason1.BackgroundTransparency = 1.000
	Reason1.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Reason1.BorderSizePixel = 0
	Reason1.Position = UDim2.new(0.258695662, 0, 0.20408164, 0)
	Reason1.Size = UDim2.new(0, 200, 0, 29)
	Reason1.Font = Enum.Font.SourceSansLight
	Reason1.Text = "Script Not Found"
	Reason1.TextColor3 = Color3.fromRGB(0, 0, 0)
	Reason1.TextScaled = true
	Reason1.TextSize = 14.000
	Reason1.TextWrapped = true

	Reason.Name = "Reason"
	Reason.Parent = Error_2
	Reason.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Reason.BackgroundTransparency = 1.000
	Reason.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Reason.BorderSizePixel = 0
	Reason.Position = UDim2.new(0, 0, 0.734693885, 0)
	Reason.Size = UDim2.new(0, 460, 0, 65)
	Reason.Font = Enum.Font.SourceSansSemibold
	Reason.Text = "Symphony Hub Has Been Discontinued."
	Reason.TextColor3 = Color3.fromRGB(0, 0, 0)
	Reason.TextScaled = true
	Reason.TextSize = 14.000
	Reason.TextWrapped = true

	Close.Name = "Close"
	Close.Parent = Error_2
	Close.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Close.BackgroundTransparency = 1.000
	Close.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Close.BorderSizePixel = 0
	Close.Position = UDim2.new(0.9152174, 0, -0.159183666, 0)
	Close.Size = UDim2.new(0, 39, 0, 39)
	Close.Image = "rbxassetid://16167590639"
	Close.ImageColor3 = Color3.fromRGB(79, 79, 79)
	Close.ImageRectOffset = Vector2.new(248, 386)
	Close.ImageRectSize = Vector2.new(36, 36)

	TitleScript.Name = "Title Script"
	TitleScript.Parent = Error_2
	TitleScript.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TitleScript.BackgroundTransparency = 1.000
	TitleScript.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TitleScript.BorderSizePixel = 0
	TitleScript.Position = UDim2.new(0.630434811, 0, 0.0571428575, 0)
	TitleScript.Size = UDim2.new(0, 159, 0, 22)
	TitleScript.Font = Enum.Font.SourceSans
	TitleScript.Text = "Symphony Hub"
	TitleScript.TextColor3 = Color3.fromRGB(0, 0, 0)
	TitleScript.TextScaled = true
	TitleScript.TextSize = 14.000
	TitleScript.TextWrapped = true

	-- Scripts:

	local function ZYYO_fake_script() -- Title404.Rainbow 
		local script = Instance.new('LocalScript', Title404)

		local textLabel = script.Parent
		local tweenService = game:GetService("TweenService")

		local colors = {
			Color3.fromRGB(255, 0, 0),
			Color3.fromRGB(255, 127, 0),
			Color3.fromRGB(255, 255, 0),
			Color3.fromRGB(0, 255, 0),
			Color3.fromRGB(0, 0, 255),
			Color3.fromRGB(75, 0, 130),
			Color3.fromRGB(148, 0, 211)
		}

		local changeInterval = 2
		local colorIndex = 1

		local function changeColor()
			local goal = {TextColor3 = colors[colorIndex]}
			local tweenInfo = TweenInfo.new(changeInterval, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
			local tween = tweenService:Create(textLabel, tweenInfo, goal)
			tween:Play()

			colorIndex = colorIndex % #colors + 1
		end

		while true do
			changeColor()
			wait(changeInterval)
		end

	end
	coroutine.wrap(ZYYO_fake_script)()
	local function NGVFQMS_fake_script() -- Close.LocalScript 
		local script = Instance.new('LocalScript', Close)

		local imageButton = script.Parent 
		local screenGui = imageButton.Parent 

		imageButton.MouseButton1Click:Connect(function()
			screenGui:Destroy() 
		end)

	end
	coroutine.wrap(NGVFQMS_fake_script)()
	local function QFJI_fake_script() -- TitleScript.Rainbow 
		local script = Instance.new('LocalScript', TitleScript)

		local textLabel = script.Parent
		local tweenService = game:GetService("TweenService")

		local colors = {
			Color3.fromRGB(255, 0, 0),
			Color3.fromRGB(255, 127, 0),
			Color3.fromRGB(255, 255, 0),
			Color3.fromRGB(0, 255, 0),
			Color3.fromRGB(0, 0, 255),
			Color3.fromRGB(75, 0, 130),
			Color3.fromRGB(148, 0, 211)
		}

		local changeInterval = 2
		local colorIndex = 1

		local function changeColor()
			local goal = {TextColor3 = colors[colorIndex]}
			local tweenInfo = TweenInfo.new(changeInterval, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
			local tween = tweenService:Create(textLabel, tweenInfo, goal)
			tween:Play()

			colorIndex = colorIndex % #colors + 1
		end

		while true do
			changeColor()
			wait(changeInterval)
		end

	end
	coroutine.wrap(QFJI_fake_script)()
	local function SOWCCN_fake_script() -- Error_2.Bootup 
		local script = Instance.new('LocalScript', Error_2)

		local screenGui = script.Parent
		local bootingSound = screenGui:FindFirstChildOfClass("Sound")
		if bootingSound then
			bootingSound:Play()
		end
		screenGui:GetPropertyChangedSignal("Parent"):Connect(function()
			if screenGui.Parent == game.Players.LocalPlayer:WaitForChild("PlayerGui") then
				bootingSound:Play()
			end
		end)


	end
	coroutine.wrap(SOWCCN_fake_script)()
	local function YTTIS_fake_script() -- Error_2.Drag 
		local script = Instance.new('LocalScript', Error_2)

		local TweenService = game:GetService("TweenService")
		local UserInputService = game:GetService("UserInputService")

		local frame = script.Parent

		local dragging = false
		local dragInput, mousePos, framePos
		local lastMousePos, lastGoalPos
		local dragSpeed = 0.069 -- smoothness :)

		local function lerp(a, b, m)
			return a + (b - a) * m
		end

		local function update(input)
			local delta = input.Position - mousePos
			local newPos = UDim2.new(
				framePos.X.Scale,
				framePos.X.Offset + delta.X,
				framePos.Y.Scale,
				framePos.Y.Offset + delta.Y
			)
			lastGoalPos = newPos
		end

		frame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				dragging = true
				mousePos = input.Position
				framePos = frame.Position
				lastMousePos = mousePos
				lastGoalPos = frame.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)

		UserInputService.InputChanged:Connect(function(input)
			if dragging and input == dragInput then
				update(input)
			end
		end)

		game:GetService("RunService").RenderStepped:Connect(function()
			if dragging then
				frame.Position = UDim2.new(
					lerp(frame.Position.X.Scale, lastGoalPos.X.Scale, dragSpeed),
					lerp(frame.Position.X.Offset, lastGoalPos.X.Offset, dragSpeed),
					lerp(frame.Position.Y.Scale, lastGoalPos.Y.Scale, dragSpeed),
					lerp(frame.Position.Y.Offset, lastGoalPos.Y.Offset, dragSpeed)
				)
			end
		end)

	end
	coroutine.wrap(YTTIS_fake_script)()
  	end
})
